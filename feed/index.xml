---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Josh Davis</title>
    <link href="http://joshldavis.com/feed/" rel="self"/>
    <link href="http://joshldavis.com/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://joshldavis.com/</id>
    <author>
        <name>Josh Davis</name>
        <email>josh@joshldavis.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://joshldavis.com{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://joshldavis.com{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>
